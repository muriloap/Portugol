programa {
  funcao inicio() {
   inteiro a, b, quadrado1, quadrado2, soma

   escreva("\nDigite o primeiro número: ")
   leia(a)

    escreva("\nDigite o segundo número: ")
   leia(b)

   quadrado1 = a * a
   quadrado2 = b * b

   soma = quadrado1 + quadrado2

   escreva("\nA soma dos quadrados de ", a, " e ", b, " é igual a: ", soma)

  }
}
